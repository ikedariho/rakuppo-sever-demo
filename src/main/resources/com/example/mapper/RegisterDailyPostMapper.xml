<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.RegisterDailyPostMapper">
	<insert id="insertDailyPost">
		INSERT INTO daily_posts (user_id,date,register_user_id,update_date,version,status) VALUES (#{userId},#{date},#{registerUserId},#{updateDate},#{version},#{status});
	</insert>
</mapper>